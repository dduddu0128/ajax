<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ajaxPrj</display-name>
   <welcome-file-list>
    <welcome-file>/board/list.jsp</welcome-file>
  </welcome-file-list>
    <servlet>
  		<servlet-name>jsonList</servlet-name>
  		<servlet-class>com.bit.ajax.controller.ListController</servlet-class>
 	 </servlet>
    <servlet>
  		<servlet-name>jsonInsert</servlet-name>
  		<servlet-class>com.bit.ajax.controller.AddController</servlet-class>
 	 </servlet>
    <servlet>
  		<servlet-name>jsonDetail</servlet-name>
  		<servlet-class>com.bit.ajax.controller.DetailController</servlet-class>
 	 </servlet>
    <servlet>
  		<servlet-name>jsonUpdate</servlet-name>
  		<servlet-class>com.bit.ajax.controller.UpdateController</servlet-class>
 	 </servlet>
    <servlet>
  		<servlet-name>jsonDelete</servlet-name>
  		<servlet-class>com.bit.ajax.controller.DeleteController</servlet-class>
 	 </servlet>
  <servlet-mapping>
  	<servlet-name>jsonList</servlet-name>
  	<url-pattern>/board/list.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>jsonInsert</servlet-name>
  	<url-pattern>/board/add.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>jsonDetail</servlet-name>
  	<url-pattern>/board/detail.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>jsonUpdate</servlet-name>
  	<url-pattern>/board/update.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>jsonDelete</servlet-name>
  	<url-pattern>/board/delete.do</url-pattern>
  </servlet-mapping>
 
</web-app>
